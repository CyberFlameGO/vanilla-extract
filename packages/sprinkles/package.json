{
  "name": "@vanilla-extract/sprinkles",
  "version": "1.5.0",
  "description": "Zero-runtime atomic CSS framework for vanilla-extract",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vanilla-extract.git",
    "directory": "packages/sprinkles"
  },
  "license": "MIT",
  "author": "SEEK",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./createRuntimeSprinkles": {
      "import": {
        "types": "./createRuntimeSprinkles/index.cjs.d.ts",
        "default": "./createRuntimeSprinkles/index.mjs"
      },
      "require": {
        "types": "./createRuntimeSprinkles/index.cjs.d.ts",
        "default": "./createRuntimeSprinkles/index.cjs"
      }
    },
    "./createUtils": {
      "import": {
        "types": "./createUtils/index.cjs.d.ts",
        "default": "./createUtils/index.mjs"
      },
      "require": {
        "types": "./createUtils/index.cjs.d.ts",
        "default": "./createUtils/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "/createRuntimeSprinkles",
    "/createUtils",
    "/dist"
  ],
  "scripts": {
    "build": "crackle package"
  },
  "devDependencies": {
    "@crackle/cli": "^0.8.0",
    "@vanilla-extract/css": "^1.6.8"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "^1.0.0"
  }
}
