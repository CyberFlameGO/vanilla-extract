{
  "name": "@vanilla-extract/css",
  "version": "1.9.0",
  "description": "Zero-runtime Stylesheets-in-TypeScript",
  "sideEffects": true,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": {
    "./dist/vanilla-extract-css.cjs.js": "./dist/vanilla-extract-css.browser.cjs.js",
    "./dist/vanilla-extract-css.esm.js": "./dist/vanilla-extract-css.browser.esm.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./adapter": {
      "import": {
        "types": "./adapter/dist/index.cjs.d.ts",
        "default": "./adapter/dist/index.mjs"
      },
      "require": {
        "types": "./adapter/dist/index.cjs.d.ts",
        "default": "./adapter/dist/index.cjs"
      }
    },
    "./disableRuntimeStyles": {
      "import": {
        "types": "./disableRuntimeStyles/dist/index.cjs.d.ts",
        "default": "./disableRuntimeStyles/dist/index.mjs"
      },
      "require": {
        "types": "./disableRuntimeStyles/dist/index.cjs.d.ts",
        "default": "./disableRuntimeStyles/dist/index.cjs"
      }
    },
    "./fileScope": {
      "import": {
        "types": "./fileScope/dist/index.cjs.d.ts",
        "default": "./fileScope/dist/index.mjs"
      },
      "require": {
        "types": "./fileScope/dist/index.cjs.d.ts",
        "default": "./fileScope/dist/index.cjs"
      }
    },
    "./functionSerializer": {
      "import": {
        "types": "./functionSerializer/dist/index.cjs.d.ts",
        "default": "./functionSerializer/dist/index.mjs"
      },
      "require": {
        "types": "./functionSerializer/dist/index.cjs.d.ts",
        "default": "./functionSerializer/dist/index.cjs"
      }
    },
    "./injectStyles": {
      "import": {
        "types": "./injectStyles/dist/index.cjs.d.ts",
        "default": "./injectStyles/dist/index.mjs"
      },
      "require": {
        "types": "./injectStyles/dist/index.cjs.d.ts",
        "default": "./injectStyles/dist/index.cjs"
      }
    },
    "./recipe": {
      "import": {
        "types": "./recipe/dist/index.cjs.d.ts",
        "default": "./recipe/dist/index.mjs"
      },
      "require": {
        "types": "./recipe/dist/index.cjs.d.ts",
        "default": "./recipe/dist/index.cjs"
      }
    },
    "./transformCss": {
      "import": {
        "types": "./transformCss/dist/index.cjs.d.ts",
        "default": "./transformCss/dist/index.mjs"
      },
      "require": {
        "types": "./transformCss/dist/index.cjs.d.ts",
        "default": "./transformCss/dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "crackle package"
  },
  "files": [
    "/adapter",
    "/disableRuntimeStyles",
    "/dist",
    "/fileScope",
    "/functionSerializer",
    "/injectStyles",
    "/recipe",
    "/transformCss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vanilla-extract.git",
    "directory": "packages/css"
  },
  "author": "SEEK",
  "license": "MIT",
  "dependencies": {
    "@emotion/hash": "^0.8.0",
    "@vanilla-extract/private": "^1.0.3",
    "chalk": "^4.1.1",
    "css-what": "^5.0.1",
    "cssesc": "^3.0.0",
    "csstype": "^3.0.7",
    "deep-object-diff": "^1.1.0",
    "deepmerge": "^4.2.2",
    "escape-string-regexp": "^4.0.0",
    "media-query-parser": "^2.0.2",
    "outdent": "^0.8.0"
  },
  "devDependencies": {
    "@crackle/cli": "publish-snapshot",
    "@types/cssesc": "^3.0.0"
  }
}
