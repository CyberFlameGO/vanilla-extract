  "description": "Zero-runtime Stylesheets-in-TypeScript",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "crackle package"
  },
  "repository": {
    "type": "git",
