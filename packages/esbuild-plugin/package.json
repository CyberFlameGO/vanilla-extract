    "directory": "packages/esbuild-plugin"
  },
  "author": "SEEK",
  "license": "MIT",
  "dependencies": {
    "@vanilla-extract/integration": "^5.0.0"
  },
  "devDependencies": {
    "@crackle/cli": "publish-snapshot",
    "esbuild": "^0.11.16"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs"
      }
