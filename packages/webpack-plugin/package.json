{
  "name": "@vanilla-extract/webpack-plugin",
  "version": "2.1.12",
  "description": "Zero-runtime Stylesheets-in-TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vanilla-extract.git",
    "directory": "packages/webpack-plugin"
  },
  "license": "MIT",
  "author": "SEEK",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./loader": {
      "import": {
        "types": "./loader/index.cjs.d.ts",
        "default": "./loader/index.mjs"
      },
      "require": {
        "types": "./loader/index.cjs.d.ts",
        "default": "./loader/index.cjs"
      }
    },
    "./virtualFileLoader": {
      "import": {
        "types": "./virtualFileLoader/index.cjs.d.ts",
        "default": "./virtualFileLoader/index.mjs"
      },
      "require": {
        "types": "./virtualFileLoader/index.cjs.d.ts",
        "default": "./virtualFileLoader/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "/dist",
    "/loader",
    "/virtualFileLoader",
    "extracted.js"
  ],
  "scripts": {
    "build": "crackle package"
  },
  "dependencies": {
    "@vanilla-extract/integration": "^5.0.0",
    "chalk": "^4.1.1",
    "debug": "^4.3.1",
    "loader-utils": "^2.0.0"
  },
  "devDependencies": {
    "@crackle/cli": "^0.8.0",
    "@types/debug": "^4.1.5",
    "@types/tapable": "~2.2.1",
    "webpack": "^5.36.1"
  },
  "peerDependencies": {
    "webpack": "^4.30.0 || ^5.20.2"
  }
}
