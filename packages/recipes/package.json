{
  "name": "@vanilla-extract/recipes",
  "version": "0.2.5",
  "description": "Create multi-variant styles with a type-safe runtime API, heavily inspired by https://stitches.dev",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "files": [
    "/createRuntimeFn",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/vanilla-extract.git",
    "directory": "packages/recipes"
  },
  "scripts": {
    "build": "crackle package"
  },
  "author": "SEEK",
  "license": "MIT",
  "peerDependencies": {
    "@vanilla-extract/css": "^1.0.0"
  },
  "devDependencies": {
    "@vanilla-extract/css": "^1.7.1",
    "@crackle/cli": "publish-snapshot"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.cjs.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./createRuntimeFn": {
      "import": {
        "types": "./createRuntimeFn/index.cjs.d.ts",
        "default": "./createRuntimeFn/index.mjs"
      },
      "require": {
        "types": "./createRuntimeFn/index.cjs.d.ts",
        "default": "./createRuntimeFn/index.cjs"
      }
    }
  }
}
